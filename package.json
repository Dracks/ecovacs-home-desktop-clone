{
  "name": "ecovacs-desktop",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.js",
    "start": "webpack --config config/webpack.js && qode ./dist/index.js",
    "start:debug": "webpack --config config/webpack.js && qode --inspect ./dist/index.js",
    "start:dev": "concurrently --handle-input \"wait-on dist/index.js && nodemon\" \"webpack --watch --config config/webpack.js\"",
    "test": "jest --config config/jest.json",
    "test:watch": "jest --config config/jest.json --watch",
    "test:cov": "jest --config config/jest.json --coverage",
    "test:travis-cov": "jest --config config/jest.travis.json",
    "package": ""
  },
  "dependencies": {
    "@nodegui/react-nodegui": "^0.14.0",
    "bufferutil": "^4.0.3",
    "ecovacs-deebot": "^0.9.0",
    "env-paths": "^2.2.1",
    "node-machine-id": "^1.1.12",
    "react": "^16.14.0",
    "react-async-hook": "^4.0.0",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@nodegui/packer": "^1.4.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.31",
    "@types/react": "^16.9.49",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.0",
    "coveralls": "^3.1.0",
    "file-loader": "^6.2.0",
    "jest": "^24.9.0",
    "jest-create-mock-instance": "^1.1.0",
    "native-addon-loader": "^2.0.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^24.0.2",
    "typescript": "^4.2.2",
    "wait-on": "^5.3.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
}
